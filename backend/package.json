{
  "name": "app-live-server",
  "version": "1.0.0",
  "description": "Backend server for app-live with Stripe Connect",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "dev:full": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "dev:ts": "ts-node server.ts",
    "vercel:dev": "vercel dev --listen 8080 --yes"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.918.0",
    "@aws-sdk/client-s3": "^3.913.0",
    "@clerk/express": "^1.7.29",
    "@supabase/supabase-js": "^2.57.4",
    "@types/nodemailer": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "fast-fuzzy": "^1.12.0",
    "fast-xml-parser": "^4.5.3",
    "ibantools": "^4.5.1",
    "multer": "^2.0.2",
    "node-fetch": "3.3.2",
    "nodemailer": "^7.0.6",
    "slugify": "^1.6.6",
    "stripe": "18.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
