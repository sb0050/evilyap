{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.ts"
    }
  ],
  "devCommand": "npm run dev",

  "env": {
    "PORT": "8080",
    "CLIENT_URL": "https://paylive-client.vercel.app"
  }
}