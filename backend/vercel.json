{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.ts"
    }
  ],
  "devCommand": "npm run dev",

  "env": {
    "PORT": "8080",
    "CLIENT_URL": "https://app-live-frontend.vercel.app",
    "STRIPE_SECRET_KEY": "${STRIPE_SECRET_KEY}",
    "STRIPE_WEBHOOK_SECRET": "${STRIPE_WEBHOOK_SECRET}",
    "CLERK_SECRET_KEY": "${CLERK_SECRET_KEY}",
    "BOXTAL_ACCESS_KEY": "${BOXTAL_ACCESS_KEY}",
    "BOXTAL_SECRET_KEY": "${BOXTAL_SECRET_KEY}"
  }
}